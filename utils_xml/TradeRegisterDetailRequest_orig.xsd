            </xs:annotation>
            <xs:restriction base="xs:boolean">
              <xs:annotation>
                <xs:documentation xml:lang="cs">[true] Pouze platné ; [false] Platné i neplatné</xs:documentation>
                <xs:documentation xml:lang="en">[true] Actual only ; [false] All historical data</xs:documentation>
                <xs:documentation>Default: true</xs:documentation>
              </xs:annotation>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="ReportType" minOccurs="0" maxOccurs="1" >
          <xs:simpleType >
            <xs:annotation>
              <xs:documentation xml:lang="cs">Typ výpisu</xs:documentation>

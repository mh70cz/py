              <xs:documentation>default = 1</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
              <xs:enumeration value="1">
                <xs:annotation>
                  <xs:documentation xml:lang="cs">Výpis bez historie</xs:documentation>
                  <xs:documentation xml:lang="en">Statement without history</xs:documentation>
                </xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="2">
                <xs:annotation>
                  <xs:documentation xml:lang="cs">Výpis s historií</xs:documentation>
                  <xs:documentation xml:lang="en">Statement with history</xs:documentation>
                </xs:annotation>
              </xs:enumeration>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
